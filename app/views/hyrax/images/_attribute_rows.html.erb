<% dynamic_schema_service = dynamic_schema_helper(AdminSet.where(title: presenter.admin_set).first.id, 
'Image',
presenter.dynamic_schema )%>

<% dynamic_schema_service.view_properties.each_pair do | prop_key, prop_value | %>
